<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="SellPhoneApplication.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:SellPhoneApplication"
    xmlns:views="clr-namespace:SellPhoneApplication.Views"
    Title="Senifone"
    >
    <ShellContent Route="LoginPage" ContentTemplate="{DataTemplate views:LoginPage}" />
    <ShellContent Route="CartPage" ContentTemplate="{DataTemplate views:CartPage}" />
    <ShellContent Route="PhonesPage" ContentTemplate="{DataTemplate views:PhonesPage}" />
    <ShellContent Route="ProductManagementPage" ContentTemplate="{DataTemplate views:ProductManagementPage}" />
    <ShellContent Route="OrderManagementPage" ContentTemplate="{DataTemplate views:OrderManagementPage}" />
    <ShellContent Route="PhoneDetailPage" ContentTemplate="{DataTemplate views:PhoneDetailPage}" />
  
    <ShellContent Route="FavouritePage" ContentTemplate="{DataTemplate views:FavouritePage}" />

   
    <ShellContent Route="RegisterPage" ContentTemplate="{DataTemplate views:RegisterPage}" />
    <ShellContent Route="HomePage" ContentTemplate="{DataTemplate views:HomePage}" />
   
</Shell>
