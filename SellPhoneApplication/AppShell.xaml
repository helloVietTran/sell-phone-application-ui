<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="SellPhoneApplication.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:SellPhoneApplication"
    xmlns:views="clr-namespace:SellPhoneApplication.View"
    Title="SellPhoneApplication">

    <ShellContent Route="PhonesPage" ContentTemplate="{DataTemplate views:PhonesPage}" />
    <ShellContent Route="HomePage" ContentTemplate="{DataTemplate views:HomePage}" />
    <ShellContent Route="LoginPage" ContentTemplate="{DataTemplate views:LoginPage}" />
    <ShellContent Route="RegisterPage" ContentTemplate="{DataTemplate views:RegisterPage}" />

</Shell>
